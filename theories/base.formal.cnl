// Formal base bundle entrypoint.
//
// This file layers a more “formal / ontology-heavy” baseline on top of the default base.
// It is intentionally explicit: if you add/remove ontologies, edit the Load list.

Load: "theories/base.cnl".

// Provenance and spatial semantics (formal, science-friendly)
Load: "theories/ontologies/w3c-prov-o/00-dictionary.generated.cnl".
Load: "theories/ontologies/w3c-prov-o/00-dictionary.extra.cnl".
Load: "theories/ontologies/w3c-prov-o/01-rules.generated.cnl".
Load: "theories/ontologies/w3c-prov-o/01-rules.extra.cnl".

Load: "theories/ontologies/geosparql/00-dictionary.generated.cnl".
Load: "theories/ontologies/geosparql/00-dictionary.extra.cnl".
Load: "theories/ontologies/geosparql/01-rules.generated.cnl".
Load: "theories/ontologies/geosparql/01-rules.extra.cnl".

// `foaf` and `wgs84` are already loaded by `theories/base.cnl`.

// Upper / foundational
Load: "theories/ontologies/dul/00-dictionary.generated.cnl".
Load: "theories/ontologies/dul/00-dictionary.extra.cnl".
Load: "theories/ontologies/dul/01-rules.generated.cnl".
Load: "theories/ontologies/dul/01-rules.extra.cnl".

Load: "theories/ontologies/bfo/00-dictionary.generated.cnl".
Load: "theories/ontologies/bfo/00-dictionary.extra.cnl".
Load: "theories/ontologies/bfo/01-rules.generated.cnl".
Load: "theories/ontologies/bfo/01-rules.extra.cnl".

Load: "theories/ontologies/ro/00-dictionary.generated.cnl".
Load: "theories/ontologies/ro/00-dictionary.extra.cnl".
Load: "theories/ontologies/ro/01-rules.generated.cnl".
Load: "theories/ontologies/ro/01-rules.extra.cnl".

Load: "theories/ontologies/iao/00-dictionary.generated.cnl".
Load: "theories/ontologies/iao/00-dictionary.extra.cnl".
Load: "theories/ontologies/iao/01-rules.generated.cnl".
Load: "theories/ontologies/iao/01-rules.extra.cnl".
