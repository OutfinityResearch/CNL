@prefix : <https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/> .
@prefix fibo-fnd-agt-agt: <https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/> rdf:type owl:Ontology ;
    rdfs:label "Financial Industry Business Ontology (Mini)" .

:Agent rdf:type owl:Class .

:AutonomousAgent rdf:type owl:Class ;
    rdfs:subClassOf :Agent .

:Person rdf:type owl:Class ;
    rdfs:subClassOf :AutonomousAgent .

:Organization rdf:type owl:Class ;
    rdfs:subClassOf :AutonomousAgent .

# Adding Financial Concepts (Technically from other modules, but consolidating for Mini)
:FinancialContract rdf:type owl:Class ;
    rdfs:label "Financial Contract" .

:Currency rdf:type owl:Class ;
    rdfs:label "Currency" .

:Corporation rdf:type owl:Class ;
    rdfs:subClassOf :Organization .

:hasIdentity rdf:type owl:ObjectProperty ;
    rdfs:domain :AutonomousAgent .
