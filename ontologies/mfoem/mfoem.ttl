@prefix : <http://purl.obolibrary.org/obo/mfoem.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.obolibrary.org/obo/mfoem.owl> rdf:type owl:Ontology ;
    rdfs:label "Emotion Ontology (Mini)" .

:Emotion rdf:type owl:Class ;
    rdfs:label "Emotion" .

:Mood rdf:type owl:Class ;
    rdfs:label "Mood" .

:AffectiveProcess rdf:type owl:Class ;
    rdfs:label "Affective Process" .

:Joy rdf:type owl:Class ;
    rdfs:subClassOf :Emotion .

:Sadness rdf:type owl:Class ;
    rdfs:subClassOf :Emotion .

:Anger rdf:type owl:Class ;
    rdfs:subClassOf :Emotion .

:Fear rdf:type owl:Class ;
    rdfs:subClassOf :Emotion .

:Surprise rdf:type owl:Class ;
    rdfs:subClassOf :Emotion .

:Disgust rdf:type owl:Class ;
    rdfs:subClassOf :Emotion .

:hasExperiencer rdf:type owl:ObjectProperty ;
    rdfs:domain :AffectiveProcess .
