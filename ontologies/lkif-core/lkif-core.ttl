:Legal_Role
    a owl:Class ;
    rdfs:comment "A legal role is a role played in a legal context. Legal role players can be both Agents and other 'things'"^^xsd:string ;
    rdfs:subClassOf role:Role, [
        a owl:Restriction ;
        owl:onProperty role:played_by ;
        owl:someValuesFrom <http://www.estrellaproject.org/lkif-core/action.owl#Agent>
    ], [
        a owl:Restriction ;
        owl:allValuesFrom <http://www.estrellaproject.org/lkif-core/action.owl#Agent> ;
        owl:onProperty role:played_by
    ] .

:Professional_Legal_Role
    a owl:Class ;
    rdfs:comment "A professional legal role is a legal profession of some person, examples: lawyer, judge etc."^^xsd:string ;
    rdfs:subClassOf :Social_Legal_Role, role:Organisation_Role, [
        a owl:Restriction ;
        owl:allValuesFrom <http://www.estrellaproject.org/lkif-core/action.owl#Person> ;
        owl:onProperty role:played_by
    ] .

:Social_Legal_Role
    a owl:Class ;
    rdfs:comment "A social legal role is played by some agent in the context of legal activities."^^xsd:string ;
    rdfs:subClassOf :Legal_Role, role:Social_Role .

role:Social_Role
    owl:disjointWith role:Function .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
