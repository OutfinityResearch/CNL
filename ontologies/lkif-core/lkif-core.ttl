@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix role: <http://www.estrellaproject.org/lkif-core/role.owl#> .
@prefix daml: <http://www.daml.org/2001/03/daml+oil#> .

action:Person
    owl:equivalentClass :Natural_Person .

<http://www.estrellaproject.org/lkif-core/legal-action.owl>
    a owl:Ontology ;
    owl:imports <http://www.estrellaproject.org/lkif-core/expression.owl> ;
    owl:versionInfo """
